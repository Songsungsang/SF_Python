rainbow = ['red', 'orange', 'yellow', 'green', 'blue', 'indigo', 'purple']
print(rainbow[2])

rainbow.sort()
print(rainbow)

rainbow.append("green")
print(rainbow)

del rainbow[3:7]
print(rainbow)