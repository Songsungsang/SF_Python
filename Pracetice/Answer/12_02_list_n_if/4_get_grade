
score = int(input("점수를 입력해 주세요 : "))

if score >= 90 and score <= 100 :
    print("A")
elif score >= 80 :
    print("B")
elif score >= 70 :
    print("C")
elif score >= 60 :
    print("D")
else :
    print("E")

